#include "src/matmul/queue_kernel_impl.h"

namespace sycldnn {
namespace matmul {
namespace internal {

template SNNStatus
queue_kernel<SNN_DATA_TYPE, SNN_INDEX_TYPE, SNN_TRANS_LHS, SNN_TRANS_RHS,
